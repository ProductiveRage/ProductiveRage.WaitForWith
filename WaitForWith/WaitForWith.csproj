<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <DocumentationFile>WaitForWith.xml</DocumentationFile>
    <Nullable>enable</Nullable>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>ProductiveRage</Authors>
    <Copyright>Copyright 2020 Productive Rage</Copyright>
    <Description>A library to make it easier to wait for the results of multiple tasks that have different return types in a convenient and type-safe manner</Description>
    <PackageTags>C# task extension WhenAll</PackageTags>
    <PackageIcon>ProductiveRageAvatar.jpg</PackageIcon>
    <RepositoryUrl>https://github.com/ProductiveRage/ProductiveRage.WaitForWith</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="ProductiveRageAvatar.jpg" Pack="true" PackagePath="ProductiveRageAvatar.jpg" Visible="false" />
    <None Include="WaitForWith.xml" Visible="false" />
  </ItemGroup>

</Project>